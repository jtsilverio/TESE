@article{freyInvestigatingAnimalActivity2017,
  title = {Investigating Animal Activity Patterns and Temporal Niche Partitioning Using Camera-Trap Data: Challenges and Opportunities},
  shorttitle = {Investigating Animal Activity Patterns and Temporal Niche Partitioning Using Camera-Trap Data},
  author = {Frey, Sandra and Fisher, Jason T. and Burton, A. Cole and Volpe, John P.},
  year = {2017},
  volume = {3},
  pages = {123--132},
  issn = {2056-3485},
  doi = {10.1002/rse2.60},
  abstract = {Time-stamped camera data are increasingly used to study temporal patterns in species and community ecology, including species' activity patterns and niche partitioning. Given the importance of niche partitioning for facilitating coexistence between sympatric species, understanding how emerging environmental stressors \textendash{} climate and landscape change, biodiversity loss and concomitant changes to community composition \textendash{} affect temporal niche partitioning is of immediate importance for advancing ecological theory and informing management decisions. A large variety of analytical approaches have been applied to camera-trap data to ask key questions about species activity patterns and temporal overlap among heterospecifics. Despite the many advances for describing and quantifying these temporal patterns, few studies have explicitly tested how interacting biotic and abiotic variables influence species' activity and capacity to segregate along the temporal niche axis. To address this gap, we suggest coordinated distributed experiments to capture sufficient camera-trap data across a range of anthropogenic stressors and community compositions. This will facilitate a standardized approach to assessing the impacts of multiple variables on species' behaviours and interactions. Ultimately, further integration of spatial and temporal analyses of camera-trap data is critical for improving our understanding of how anthropogenic activities and landscape changes are altering competitive interactions and the dynamics of animal communities.},
  annotation = {\_eprint: https://zslpublications.onlinelibrary.wiley.com/doi/pdf/10.1002/rse2.60},
  copyright = {\textcopyright{} 2017 The Authors. Remote Sensing in Ecology and Conservation published by John Wiley \& Sons Ltd on behalf of Zoological Society of London.},
  file = {/home/jeff/GoogleDrive/Papers/Frey et al_2017_Investigating animal activity patterns and temporal niche partitioning using.pdf;/home/jeff/Zotero/storage/XDBS4KB8/rse2.html},
  journal = {Remote Sensing in Ecology and Conservation},
  keywords = {Activity patterns,camera,camera trapping,competition,niche partitioning,species coexistence,species interactions,trap},
  language = {en},
  number = {3}
}


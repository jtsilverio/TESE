@article{toienHibernationBlackBears2011,
  title = {Hibernation in {{Black Bears}}: {{Independence}} of {{Metabolic Suppression}} from {{Body Temperature}}},
  shorttitle = {Hibernation in {{Black Bears}}},
  author = {T{\o}ien, {\O}ivind and Blake, John and Edgar, Dale M. and Grahn, Dennis A. and Heller, H. Craig and Barnes, Brian M.},
  year = {2011},
  month = feb,
  volume = {331},
  pages = {906--909},
  publisher = {{American Association for the Advancement of Science}},
  issn = {0036-8075, 1095-9203},
  doi = {10.1126/science.1199435},
  abstract = {Black bears hibernate for 5 to 7 months a year and, during this time, do not eat, drink, urinate, or defecate. We measured metabolic rate and body temperature in hibernating black bears and found that they suppress metabolism to 25\% of basal rates while regulating body temperature from 30\textdegree{} to 36\textdegree C, in multiday cycles. Heart rates were reduced from 55 to as few as 9 beats per minute, with profound sinus arrhythmia. After returning to normal body temperature and emerging from dens, bears maintained a reduced metabolic rate for up to 3 weeks. The pronounced reduction and delayed recovery of metabolic rate in hibernating bears suggest that the majority of metabolic suppression during hibernation is independent of lowered body temperature. Hibernating black bears suppress their metabolic rate to 25\% of normal, but only slightly reduce their body temperature. Hibernating black bears suppress their metabolic rate to 25\% of normal, but only slightly reduce their body temperature.},
  chapter = {Report},
  copyright = {Copyright \textcopyright{} 2011, American Association for the Advancement of Science},
  file = {/home/jeff/GoogleDrive/Papers/Toien et al_2011_Hibernation in Black Bears.pdf;/home/jeff/Zotero/storage/BC3KNGJC/906.html},
  journal = {Science},
  language = {en},
  number = {6019},
  pmid = {21330544}
}

@article{tomotaniFieldLaboratoryStudies2012,
  title = {Field and {{Laboratory Studies Provide Insights}} into the {{Meaning}} of {{Day}}-{{Time Activity}} in a {{Subterranean Rodent}} ({{Ctenomys}} Aff. Knighti), the {{Tuco}}-{{Tuco}}},
  author = {Tomotani, Barbara M. and Flores, Danilo E. F. L. and Tachinardi, Patr{\'i}cia and Paliza, Jos{\'e} D. and Oda, Gisele A. and Valentinuzzi, Ver{\^o}nica S.},
  editor = {Mistlberger, Ralph E.},
  year = {2012},
  month = may,
  volume = {7},
  pages = {e37918},
  issn = {1932-6203},
  doi = {10.1371/journal.pone.0037918},
  abstract = {South American subterranean rodents (Ctenomys aff. knighti), commonly known as tuco-tucos, display nocturnal, wheelrunning behavior under light-dark (LD) conditions, and free-running periods .24 h in constant darkness (DD). However, several reports in the field suggested that a substantial amount of activity occurs during daylight hours, leading us to question whether circadian entrainment in the laboratory accurately reflects behavior in natural conditions. We compared circadian patterns of locomotor activity in DD of animals previously entrained to full laboratory LD cycles (LD12:12) with those of animals that were trapped directly from the field. In both cases, activity onsets in DD immediately reflected the previous dark onset or sundown. Furthermore, freerunning periods upon release into DD were close to 24 h indicating aftereffects of prior entrainment, similarly in both conditions. No difference was detected in the phase of activity measured with and without access to a running wheel. However, when individuals were observed continuously during daylight hours in a semi-natural enclosure, they emerged above-ground on a daily basis. These day-time activities consisted of foraging and burrow maintenance, suggesting that the designation of this species as nocturnal might be inaccurate in the field. Our study of a solitary subterranean species suggests that the circadian clock is entrained similarly under field and laboratory conditions and that day-time activity expressed only in the field is required for foraging and may not be time-dictated by the circadian pacemaker.},
  file = {/home/jeff/GoogleDrive/Papers/Tomotani et al_2012_Field and Laboratory Studies Provide Insights into the Meaning of Day-Time.pdf},
  journal = {PLoS ONE},
  language = {en},
  number = {5}
}


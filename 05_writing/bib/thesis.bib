@article{floresModelingNaturalPhotic2013,
  title = {Modeling {{Natural Photic Entrainment}} in a {{Subterranean Rodent}} ({{Ctenomys}} Aff. Knighti), the {{Tuco}}-{{Tuco}}},
  author = {Fl{\^o}res, Danilo E. F. L. and Tomotani, Barbara M. and Tachinardi, Patricia and Oda, Gisele A. and Valentinuzzi, Veronica S.},

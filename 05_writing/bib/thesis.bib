@article{massotONEIROSNewMiniature2019,
  title = {{{ONEIROS}}, a New Miniature Standalone Device for Recording Sleep Electrophysiology, Physiology, Temperatures and Behavior in the Lab and Field},
  author = {Massot, Bertrand and Arthaud, S{\'e}bastien and Barrillot, Baptiste and Roux, Johanna and Ungurean, Gianina and Luppi, Pierre-Herv{\'e} and Rattenborg, Niels C. and Libourel, Paul-Antoine},
  year = {2019},
  month = mar,
  volume = {316},
  pages = {103--116},
  issn = {0165-0270},
  doi = {10.1016/j.jneumeth.2018.08.030},
  abstract = {Background Sleep is an inactive state of reduced environmental awareness shared by all animals. When compared to wakefulness, sleep behavior is associated with changes in physiology and brain activity. The nature of these changes varies considerably across species, and therefore is a rich resource for gaining insight into the evolution and functions of sleep. A major obstacle to capitalizing on this resource is the lack of a small device capable of recording multiple biological parameters for extended periods of time both in the laboratory and the field. New method ONEIROS is a new tool designed for conducting sleep research on small, freely moving animals. The miniature, standalone system is capable of recording up to 26 electrophysiological signals (electroencephalogram, electromyogram, electrooculogram, electrocardiogram), metabolic (3 temperature channels) and behavior via an accelerometer for several days. In addition, the device is equipped with a vibrating motor which can be used to assess arousal thresholds and to disrupt sleep. The system is available in telemetric or data-logger configuration useable in the field. Results To demonstrate the efficacy of this tool, we simultaneously recorded for the first time, electroencephalogram, hippocampal local field potential, electromyogram, electrooculogram, brain, body and ambient temperature, and 3D accelerometry. We also deprived rats of paradoxical sleep by triggering the vibrating motor after online recognition of the state. Finally, by successfully recording a pigeon in an 8\,m3 aviary in a social context with the device in the logger configuration, we demonstrate the feasibility of using the device in the field.},
  file = {/home/jeff/GoogleDrive/Papers/Massot et al_2019_ONEIROS, a new miniature standalone device for recording sleep.pdf;/home/jeff/Zotero/storage/77E98XK5/S0165027018302668.html},
  journal = {Journal of Neuroscience Methods},
  keywords = {Data-logger,Electrophysiology,Pigeon,Sleep,Sleep deprivation,Telemetry,Wireless},
  language = {en},
  series = {Methods and Models in Sleep Research: {{A Tribute}} to {{Vincenzo Crunelli}}}
}

@article{mateoValidationExtractionMethods2005,

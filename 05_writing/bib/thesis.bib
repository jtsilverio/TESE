@article{pageInteractionsBilaterallyPaired1978,
  title = {Interactions between Bilaterally Paired Components of the Cockroach Circadian System},
  author = {Page, Terry L.},
  year = {1978},
  volume = {124},
  pages = {225--236},
  issn = {0340-7594, 1432-1351},
  doi = {10.1007/BF00657054},
  abstract = {The hypothesis that bilaterally redundant, and mutually coupled circadian pacemakers are located near the lobula of the optic lobes of the cockroach, Leucophaea rnaderae, was investigated in a series of lesion experiments. 1. Following removal of one optic lobe electrolytic lesions near the lobula in the dorsal 1/3 of the remaining lobe had no effect on the activity rhythm or its entrainment to a light cycle. Lesions in the ventral 2/3 of the lobula region either had no effect or resulted in aperiodic activity (Figs. 3, 4).},
